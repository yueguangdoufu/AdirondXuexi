<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btnSax"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="30dp"
        android:text="SAX解析"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnDom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="37dp"
        android:text="Dom解析"
        app:layout_constraintStart_toEndOf="@+id/btnPull"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnPull"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:text="Pull解析"
        app:layout_constraintBaseline_toBaselineOf="@+id/btnPull2"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnPull2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:text="pull生成"
        app:layout_constraintStart_toStartOf="@+id/btnDom"
        app:layout_constraintTop_toBottomOf="@+id/btnDom" />

    <ListView
        android:layout_width="368dp"
        android:layout_height="386dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="173dp" />

</android.support.constraint.ConstraintLayout>